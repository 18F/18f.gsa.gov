require 'yaml'

def match_date(str)
  begin
    str.match(/(\d{4}-\d{1,2}-\d{1,2})/).captures.first
  rescue NoMethodError
    false
  end
end

Dir["content/posts/*.md"].each do |path|
  date_line = File.open(path).each_line.grep(/^date:/).first
  puts "No date for #{path}\n\n" if date_line.nil?
  next if date_line.nil?

  date_in_file = Date.parse(match_date(date_line)).to_s
  date_in_path = Date.parse(match_date(path)).to_s

  unless date_in_file == date_in_path
    puts <<~MSG
      Date in file #{path} is not the same
        file: #{date_in_file}
        path: #{date_in_path}

    MSG
  end
rescue => e
  puts path
  raise e
end
